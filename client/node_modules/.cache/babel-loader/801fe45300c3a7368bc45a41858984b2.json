sistedstate'\nimport { UPDATE_CURRENT, UPDATE_BOOK, DELETE_BOOK } from './mutation-types'\n//追加\nimport axios from \"axios\";\n\nVue.use(Vuex)\n\nexport default new Vuex.Store({\n  strict: true,\n  state: {\n   //books: [],\n   current: null\n  },\n  getters: {\n   //追加部分\n   books(){\n    let book = []\n    //this.$http('http://localhost:8888/list')\n     axios.get('http://localhost:8888/list')\n     .then( (response) => {\n      return response.json()\n     })\n     .then( ( data) => {\n      for( let b of data.items){\n       book.push(b)\n      }\n     })\n    return book\n   },\n\n   bookCount( ) {\n    return this.books().length\n   },\n   allBooks( ) {\n    return this.books()\n   },\n   getRangeByPage( ) {\n    return page => {\n     const SIZE = 3\n     return this.books().slice( (page-1)*SIZE , (page-1)*SIZE+SIZE )\n    }\n   },\n   getBookById( ) {\n    return id => {\n     return this.books().find( book => book.id == id )\n    }\n   },\n   current( state ) {\n    return state.current;\n   }\n  },\n  mutations: {\n   [ UPDATE_CURRENT ]( state , payload ) {\n    state.current = payload\n   },\n   [ UPDATE_BOOK ]( state , payload ){\n    let b = this.getters.getBookById( payload.id )\n    if (b) {\n     Object.assign(b, payload)\n    } else {\n     //state.books.push(payload)\n    }\n   },\n   [ DELETE_BOOK ]( state , payload ){\n    let b = this.getters.getBookById( payload.id )\n    if (b) {\n     //state.books = state.books.filter( book => book.id !== b.id )\n    }\n   }\n  },\n  actions: {\n   [UPDATE_CURRENT]( { commit }, payload ) {\n    commit( UPDATE_CURRENT, payload)\n   },\n   [UPDATE_BOOK]( {commit} , payload){\n    commit( UPDATE_BOOK , payload )\n   },\n   [DELETE_BOOK]( {commit} , payload){\n    commit( DELETE_BOOK , payload )\n   }\n  },\n  plugins: [createPersistedState({\n   key: 'reading-recorder',\n   storage: localStorage\n  })],\n  modules: {\n  }\n})\n"]}]}